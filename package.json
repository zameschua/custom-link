{
  "name": "custom-link",
  "version": "1.0.0",
  "description": "A web application / REST Api for users to create custom urls",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "client": "cd client && yarn start",
    "server": "nodemon index.js",
